include ../Makefile.com
include ../../Makefile.iffe

FEATURE/acct FEATURE/execargs FEATURE/pstat:
	PATH=$(AST_TOOLS):$$PATH \
	    $(AST_IFFE) $(IFFEFLAGS) -c '$(IFFEC)' def $(@F)
FEATURE/math := IFFE_EXTRA = $(ASTSRC)/data/math.tab

cleaniffe: FRC
	$(RM) $(FEATURES:%=FEATURE/%)

generate: cleaniffe $(FEATURES:%=FEATURE/%)
